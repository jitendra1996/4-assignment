<%- include('includes/head.ejs') %>
    <link rel="stylesheet" href="/css/list.css">
</head>
<body>
    <%- include('includes/navigation.ejs') %>
    <div class="list">
        <% if( names.length > 0) { %>
            <ul>
                <% for (let name of names) { %>
                    <li class="list-content"><%= name.username %></li>
                <% } %>
            </ul>
        <% }else { %>
            <h1>No names</h1>
        <% } %>    
    </div>
<%- include('includes/end.ejs') %>